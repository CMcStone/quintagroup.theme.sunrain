/*---------- PORTAL EDIT ----------*/
body.kssActive h1.inlineEditable:hover {
  padding-bottom:0;
  }
body.kssActive abbr.inlineEditable:hover {
  padding:0 0 1px;
  }
.kupu-html body { 
	text-align:left;
	}

/*---------- PORTAL CONTENT ----------*/
.documentContent {
  background-color:&dtml-bgColorGlobal;;
  font-size:100%;
  }
.documentContent h1 {
  border-bottom:0;
  font-size:1.5em;
  }
.documentContent h2 {
  border-bottom:0;
  font-size:1.17em;
  font-weight:bold;
  }
.documentContent h3 {
  font-size:1em;
  }
#content a[href^="http:"] {
  background: 0;
  padding: 0;
  }

.documentActions {
  border-top:1px solid &dtml-globalBackgroundColor;;
  }
.documentActions ul {
  line-height:normal;
  margin:2px 0 0;
  padding:0;
  }
.documentActions li {
  background-color:&dtml-evenRowBackgroundColor;;
  margin:0 0 0 0.5em;
  padding:2px 17px;  
  }

/*---------- PORTAL FIXES ----------*/
.hiddenStructure {
  display:none;
  }
.contentActions {
  z-index:0;
  }

/*----- Search -----*/
form.searchPage {
  text-align:left;
  }
input.searchPage {
  font-size:150% !important;
  }
form.searchPage input.searchButton {
  background:&dtml-globalBackgroundColor;;
  border:0;
  color:&dtml-globalFontColor;;
  height:26px;
  margin-bottom:0 !important;
  padding:0;
  text-transform:none;
  }

