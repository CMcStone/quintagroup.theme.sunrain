/*---------- PORTAL FOOTER ----------*/
#portal-footer {
  background-color:#fff;
  border:0;
  border-top:30px solid &dtml-bgColorGlobal;;
  padding:1em 0 0;
  }
#portal-footer p {
  
  }

/*---------- PORTAL COLOPHON ----------*/
